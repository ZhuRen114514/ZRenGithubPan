    lx = gg.TYPE_DOUBLE
   elseif LX == 'W' then
    lx = gg.TYPE_WORD
   elseif LX == 'B' then
    lx = gg.TYPE_BYTE
   elseif LX == 'Q' then
    lx = gg.TYPE_QWORD
   elseif LX == 'X' then
    lx = gg.TYPE_XOR
